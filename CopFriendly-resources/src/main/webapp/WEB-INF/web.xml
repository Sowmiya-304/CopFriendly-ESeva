<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginController</servlet-name>
  	<display-name>LoginController</display-name>
  	<description></description>
  	<servlet-class>com.CopFriendly.Controller.LoginController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegisterViolationController</servlet-name>
  	<display-name>RegisterViolationController</display-name>
  	<description></description>
  	<servlet-class>com.CopFriendly.Controller.RegisterViolationController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegisterEmergencyController</servlet-name>
  	<display-name>RegisterEmergencyController</display-name>
  	<description></description>
  	<servlet-class>com.CopFriendly.Controller.RegisterEmergencyController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ForgotPasswordController</servlet-name>
  	<display-name>ForgotPasswordController</display-name>
  	<description></description>
  	<servlet-class>com.CopFriendly.Controller.ForgotPasswordController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginController</servlet-name>
  	<url-pattern>/LoginController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterViolationController</servlet-name>
  	<url-pattern>/RegisterViolationController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterEmergencyController</servlet-name>
  	<url-pattern>/RegisterEmergencyController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ForgotPasswordController</servlet-name>
  	<url-pattern>/ForgotPasswordController</url-pattern>
  </servlet-mapping>
</web-app>
